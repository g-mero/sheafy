{
  "$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
  "extends": ["ultracite/core", "ultracite/solid"],
  "linter": {
    "rules": {
      "nursery": "off",
      "suspicious": {
        "noExplicitAny": "warn",
        "noConsole": "warn"
      },
      "performance": {
        "noBarrelFile": "off",
        "useTopLevelRegex": "off"
      }
    }
  }
}
